from tkinter import *
from tkinter.ttk import *
window = Tk()
window.title("Programming TT 2.0")
window.geometry("300x300")

def clicked():
    lb = Label(window, text="Hello World", font=("Arial Bold", 17))
    lb.pack(pady=10)


bt = Button(window, text="Enter", command=clicked)
bt.pack(pady=20)

window.mainloop()
